			<script>
				// Wait for the DOM to be fully loaded
				$(document).ready(function () {

					// Select the button(s) you want to attach the event listener to
					const removeButtons = document.querySelectorAll('.removebtn');
					// Loop through each button and attach the event listener
					removeButtons.forEach(function (button) {
						button.addEventListener('click', function (event) {
							event.preventDefault(); // Prevent the default button behavior
							const coupon = document.getElementById('coupon').value

							const productId = $(button).data('product-id');
							console.log(productId);

							const productContainer = button.closest(".product-cart, .product-name"); // Store the product container
							$.ajax({
								url: `/removeFromCart/${productId}`,
								type: 'DELETE',
								data: { coupon: coupon },
								success: function (data) {
									console.log("success response", data)
									if (data.success) {
										// Display a SweetAlert2 success notification
										Swal.fire({
											position: 'center',
											icon: 'success',
											title: 'Removed from cart',
											showConfirmButton: false,
											timer: 1100
										});
										productContainer.remove();
										updateCartTotal(data);
									} else {
										console.error('Failed to remove item from cart');
									}
								},
								error: function (error) {
									console.error('Error occurred while removing item from cart', error);
								}
							});
						});
					});
				});


				function updateCartTotal({  cart }) {
					const delivery = 30;
					const discount = cart.couponDiscount.toFixed(2)
					const subtotal = cart.cartSubtotal.toFixed(2);
					const total = (parseFloat(subtotal-discount) + delivery).toFixed(2); // Convert subtotal to a number

					$('#cartSubtotalValue').text(`₹${subtotal}`);
					$('#discount').text(`₹${discount}`);
					$('.grand-total').text(`₹${total}`);
				}


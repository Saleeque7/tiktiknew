			<script>
				function redirectToCheckout(productId){
					console.log(productId,"productId");
					const coupon = document.getElementById('coupon').value	
					const encryptedCoupon = CryptoJS.AES.encrypt(coupon, 'secretKey').toString();
		console.log(coupon,"coupon");
		let requestData = { 
        coupon: encryptedCoupon, 
        };
		let queryString = $.param(requestData);


		let urlWithParams = '/checkout?' + queryString;

		$.ajax({
						url: '/check-quantity',
						type: 'POST',
						contentType: 'application/json',
						data: JSON.stringify({
							couponId :coupon
						}),				
						success: function (data) {
							if (data.success) {
								window.location = urlWithParams

							} else {
								Swal.fire({
									position: "center",
									icon: "error",
									text: data.message || "An error occurred",
									showConfirmButton: false,
									timer: 1100
								});
								if(data.cart){
									updateddPrice(data)
								}
							}
						},
						error: function (error) {
							console.error('Error:', error);
							Swal.fire({
								icon: "error",
								text: "An error occurred"
							});
						}
					});
				}

				function updateddPrice({cart}) {
					const discount = cart.couponDiscount
					const delivery = 30 
					const carttotal = cart.cartSubtotal.toFixed(2);
					const total = (parseFloat(carttotal) + delivery).toFixed(2);
				

					$('#discount').html(`<span>Discount:</span><span id="discount">₹${discount}</span>`);
					$('.grand-total').text(`₹${total}`);

				}
			</script>

			<script>


				$(document).ready(function () {
					$('.decrement-button').click(function () {
						updateQuantity($(this).data('product-id'), 'decrement');
					});

					$('.increment-button').click(function () {
						updateQuantity($(this).data('product-id'), 'increment');
					});

					// Listen to input changes to update the price when the quantity changes
					$('.quantity-input').change(function () {
						const productId = $(this).data('product-id');
						updatePrice(productId);
					});
				});

				function updateQuantity(productId, operation) {
					const coupon = document.getElementById('coupon').value
					const quantityInput = $(`#quantityInput${productId}`);
					let currentQuantity = parseInt(quantityInput.val(), 10);

					if (operation === 'decrement') {
						currentQuantity = Math.max(currentQuantity - 1, 1);
					} else if (operation === 'increment') {
						currentQuantity += 1;
					}

					quantityInput.val(currentQuantity);

					// Update the associated price
					$.ajax({
						url: `/update-quantity/${operation}`,
						type: 'POST',
						contentType: 'application/json',
						data: JSON.stringify({
							productId: productId,
							quantity: currentQuantity,
							couponId :coupon
						}),
						success: function (data) {
							if (data.success) {
								updatePrice(productId);
								updatedCartTotal(data);
							} else {
								Swal.fire({
									position: "center",
									icon: "error",
									text: data.message || "An error occurred",
									showConfirmButton: false,
									timer: 1100
								});
							}
						},
						error: function (error) {
							console.error('Error:', error);
							Swal.fire({
								icon: "error",
								text: "An error occurred"
							});
						}
					});
				}
				function applyCoupon() {

        const coupon = document.getElementById('coupon').value
		const couponError = document.getElementById('couponError')
		const couponsuccess = document.getElementById('couponsuccess')
		
		$.ajax({
    url: '/userapplycoupon',
    type: 'post',
    data: {
        couponCode: coupon,
    },
    success: function (data) {
        if (data.success) {
            updatedCartTotal(data);
            document.getElementById('availableCoupon').style.display = 'none';
            console.log(data);
            couponsuccess.innerHTML = `*${data.message}`;
            updateButton();
        } else {
            couponError.innerHTML = `*${data.message}`;
            updatedCartTotal(data);
        }

        setTimeout(() => {
            couponError.innerHTML = '';
            couponsuccess.innerHTML = '';
        }, 2000);
    }
})
}
				function updatePrice(productId) {
					const quantityInput = $(`#quantityInput${productId}`);
					const priceElement = $(`.price[data-product-id="${productId}"]`);
					const salesPrice = parseFloat(priceElement.data('sales-price'));
					const quantity = parseInt(quantityInput.val(), 10);
					const totalPrice = (salesPrice * quantity).toFixed(2);

					priceElement.text(`₹${totalPrice}`);
				}

